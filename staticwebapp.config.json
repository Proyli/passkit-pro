{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/favicon.ico",
      "/site.webmanifest",
      "/*.{png,jpg,jpeg,gif,ico,svg,css,js,map,txt,xml,webmanifest}"
    ]
  },
  "routes": [
    {
      "route": "/apple-touch-icon.png",
      "rewrite": "/alcazaren-favicon.jpg"
    },
    {
      "route": "/favicon.ico",
      "rewrite": "/alcazaren-favicon.jpg"
    },
    {
      "route": "/*",
      "methods": [
        "get"
      ],
      "rewrite": "/index.html",
      "allowedRoles": [
        "anonymous"
      ]
    }
  ]
}
